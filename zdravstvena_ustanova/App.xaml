<Application x:Class="zdravstvena_ustanova.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:zdravstvena_ustanova"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <materialDesign:BundledTheme x:Key="MaterialDesignBundledTheme" BaseTheme="Dark" PrimaryColor="Amber" SecondaryColor="Yellow" />
            <ResourceDictionary x:Key="MaterialDesignResourceDictionary" Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
