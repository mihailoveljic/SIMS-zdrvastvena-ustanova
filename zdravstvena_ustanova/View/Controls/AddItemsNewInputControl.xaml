<UserControl x:Class="zdravstvena_ustanova.View.Controls.AddItemsNewInputControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:zdravstvena_ustanova.View.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBox Text="{Binding ItemsForTransfer}" Margin="0,0,0,0" PreviewTextInput="QuantityTextBox_PreviewTextInput" VerticalContentAlignment="Center" x:Name="QuantityTextBox" Grid.Column="1" Grid.Row="1" Width="200" Height="28" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBox>
    </Grid>
</UserControl>
