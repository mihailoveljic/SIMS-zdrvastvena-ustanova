<Window x:Class="zdravstvena_ustanova.View.Windows.DoctorWindows.AnamnesisTextBoxInput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:zdravstvena_ustanova.View.Windows.DoctorWindows"
        mc:Ignorable="d"
        Title="AnamnesisTextBoxInput" Height="500" Width="800" WindowStartupLocation="CenterScreen">
    <WrapPanel Width="800" Height="450">
        <TextBox  x:Name="TextBoxInput" TextWrapping="Wrap" AcceptsTab="True" AcceptsReturn="False" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled" HorizontalAlignment="Center" Text="TextBox" VerticalAlignment="Top" Width="800" Height="371"/>
        <Button Content="Cancel" Width="114" Height="48" HorizontalAlignment="Center"  VerticalAlignment="Top" Margin="20,20,0,0" Click="Button_Click"/>
        <Button Content="Submit" Width="114" Height="48" HorizontalAlignment="Center"  VerticalAlignment="Top" Margin="10,20,0,0" Click="Button_Click_1"/>
    </WrapPanel>
</Window>
