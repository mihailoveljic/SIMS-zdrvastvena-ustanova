<Window x:Class="zdravstvena_ustanova.View.NoteNoti"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:zdravstvena_ustanova.View"
        mc:Ignorable="d"
        Title="Notifikacija za beleske" Height="280" Width="300" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize">
    <Grid>
        <Label Content="Naziv beleske :" HorizontalAlignment="Left" Margin="31,11,0,0" VerticalAlignment="Top"/>
        <Label Content="Sadrzaj beleske :" HorizontalAlignment="Left" Margin="31,62,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="name" HorizontalAlignment="Center" Margin="0,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="212" IsEnabled="False"/>
        <TextBox x:Name="content" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Top" Width="212" Height="74" IsEnabled="False" Margin="0,88,0,0"/>
        <Button Content="_U redu" HorizontalAlignment="Center" Margin="0,185,0,27" Width="60" Click="goBack" />

    </Grid>
</Window>
